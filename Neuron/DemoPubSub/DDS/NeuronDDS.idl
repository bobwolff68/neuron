module NeuronDDS
{
	const long	MAX_NAMELEN = 32;
	typedef string<MAX_NAMELEN>		Name;
	typedef sequence<char>			Buffer;		
	
	struct SrcAdvert
	{
		long	srcID;
		Name	srcName;
	};
#pragma keylist SrcAdvert srcID
	
	struct Frame
	{
		long	srcID;
		long	index;
		long	layerType;
		long	size;
		Buffer	payload;
	};
#pragma keylist Frame srcID

	struct ThrotMsg
	{
		long srcID;
		long mode;
	};
#pragma keylist ThrotMsg srcID
};

